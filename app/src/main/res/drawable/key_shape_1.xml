<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:constantSize="true">
    <item android:state_pressed="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <solid android:color="@color/key_background_1" />
            <stroke android:width="@dimen/key_border_weight_1" android:color="@color/key_border_shadow_1" />
            <corners android:radius="@dimen/key_border_radius_1" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <solid android:color="@color/dark_key_background_1" />
            <stroke android:width="@dimen/key_border_weight_1" android:color="@color/dark_key_border_shadow_1" />
            <corners android:radius="@dimen/key_border_radius_1" />
        </shape>
    </item>
</selector>
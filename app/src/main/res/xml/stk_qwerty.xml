<StkBoard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="@dimen/key_width_alpha_sym"
    android:horizontalGap="@dimen/hGap"
    android:verticalGap="@dimen/vGap"
    android:keyHeight="@dimen/key_height">

    <StkRow>
        <StkKey android:codes="113" android:keyLabel="q" android:keyEdgeFlags="left"/>
        <StkKey android:codes="119" android:keyLabel="w" />
        <StkKey android:codes="101" android:keyLabel="e"/>
        <StkKey android:codes="114" android:keyLabel="r"/>
        <StkKey android:codes="116" android:keyLabel="t"/>
        <StkKey android:codes="121" android:keyLabel="y"/>
        <StkKey android:codes="117" android:keyLabel="u"/>
        <StkKey android:codes="105" android:keyLabel="i"/>
        <StkKey android:codes="111" android:keyLabel="o"/>
        <StkKey android:codes="112" android:keyLabel="p" android:keyEdgeFlags="right"/>
    </StkRow>
    
    <StkRow>
        <StkKey android:codes="97" android:keyLabel="a" android:horizontalGap="@dimen/hGapR2QWERTY"
                android:keyEdgeFlags="left"/>
        <StkKey android:codes="115" android:keyLabel="s"/>
        <StkKey android:codes="100" android:keyLabel="d"/>
        <StkKey android:codes="102" android:keyLabel="f"/>
        <StkKey android:codes="103" android:keyLabel="g"/>
        <StkKey android:codes="104" android:keyLabel="h"/>
        <StkKey android:codes="106" android:keyLabel="j"/>
        <StkKey android:codes="107" android:keyLabel="k"/>
        <StkKey android:codes="108" android:keyLabel="l" android:keyEdgeFlags="right"/>
    </StkRow>
    
    <StkRow>
        <StkKey android:codes="-1" android:keyLabel="SHIFT"
                android:keyWidth="@dimen/key_width_button" android:isModifier="true"
                android:isSticky="true" android:keyEdgeFlags="left"/>
        <StkKey android:codes="122" android:keyLabel="z"/>
        <StkKey android:codes="120" android:keyLabel="x"/>
        <StkKey android:codes="99" android:keyLabel="c"/>
        <StkKey android:codes="118" android:keyLabel="v"/>
        <StkKey android:codes="98" android:keyLabel="b"/>
        <StkKey android:codes="110" android:keyLabel="n"/>
        <StkKey android:codes="109" android:keyLabel="m"/>
        <StkKey android:codes="-5" android:keyLabel="DEL"
                android:keyWidth="@dimen/key_width_button" android:keyEdgeFlags="right"
                android:isRepeatable="true"/>
    </StkRow>
    
    <StkRow android:rowEdgeFlags="bottom">
        <StkKey android:codes="-3" android:keyLabel="DONE"
                android:keyWidth="15%p" android:keyEdgeFlags="left"/>
        <StkKey android:codes="-2" android:keyLabel="123" android:keyWidth="11%p"/>
        <!--
            android:codes: -101 is not a framework-defined key code but a key code that is
            privately defined in com.example.android.softkeyboard.LatinKeyboardView.
        -->
        <Key android:codes="-101" android:keyLabel="LANG"
                android:keyWidth="12%p"/>
        <StkKey android:codes="32" android:keyLabel="SPACE"
                android:keyWidth="@dimen/key_width_space" android:isRepeatable="true"/>
        <StkKey android:codes="176" android:keyLabel="à¤•"
            android:keyWidth="11%p"/>
        <StkKey android:codes="46,44" android:keyLabel=". ,"
                android:keyWidth="11%p"/>
        <StkKey android:codes="10" android:keyLabel="RET"
                android:keyWidth="12%p" android:keyEdgeFlags="right"/>
    </StkRow>
</StkBoard>
    